{
  "name": "rule-game",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "react-app-rewired build",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject",
    "lint:css": "stylelint ./**/*.tsx",
    "now-build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "husky ": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "eslint": "6.8.0",
    "file-saver": "2.0.2",
    "grommet": "2.9.0",
    "grommet-icons": "4.4.0",
    "history": "4.9.0",
    "lodash": "4.17.15",
    "polished": "3.4.1",
    "react": "16.8.6",
    "react-dnd": "8.0.0",
    "react-dnd-html5-backend": "8.0.0",
    "react-dom": "16.8.6",
    "react-icons": "3.7.0",
    "react-redux": "7.1.0",
    "react-scripts": "3.3.0",
    "react-tooltip": "3.11.1",
    "react-use": "13.24.0",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-observable": "1.1.0",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "rxjs": "6.5.2",
    "shortid": "2.2.15",
    "styled-components": "4.3.2",
    "stylelint-webpack-plugin": "0.10.5",
    "typesafe-actions": "5.1.0",
    "typescript": "3.8.1-rc",
    "utility-types": "3.10.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.3.3",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@types/cypress-cucumber-preprocessor": "1.12.0",
    "@types/file-saver": "2.0.1",
    "@types/history": "4.7.2",
    "@types/react-redux": "7.1.1",
    "@types/react-tooltip": "3.11.0",
    "@types/redux-logger": "3.0.7",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "4.1.18",
    "@types/uuid": "3.4.6",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-plugin-styled-components": "1.10.1",
    "cross-env": "5.2.0",
    "customize-cra": "0.9.1",
    "cypress": "3.8.1",
    "cypress-cucumber-preprocessor": "1.19.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-typescript": "4.0.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-cypress": "2.6.0",
    "eslint-plugin-flowtype": "3.10.3",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "fs-extra": "8.1.0",
    "install": "0.13.0",
    "prettier": "1.19.1",
    "raw-loader": "4.0.0",
    "react-app-rewired": "2.1.3",
    "stylelint": "12.0.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.8.0",
    "ts-loader": "6.0.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
